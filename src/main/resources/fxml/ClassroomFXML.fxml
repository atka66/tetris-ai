<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="hu.atka.tetrisai.view.controllers.ClassroomController">
    <children>
        <Label layoutX="486.0" layoutY="72.0" text="Next piece:"/>
        <Label layoutX="112.0" layoutY="72.0" text="Score:"/>
        <Button fx:id="buttonLeft" layoutX="240.0" layoutY="406.0" mnemonicParsing="false" onAction="#handleButtonLeft" prefHeight="30.0"
                prefWidth="50.0" text="&lt;-"/>
        <Button fx:id="buttonRight" layoutX="350.0" layoutY="406.0" mnemonicParsing="false" onAction="#handleButtonRight" prefHeight="30.0"
                prefWidth="50.0" text="-&gt;"/>
        <Button fx:id="buttonDown" layoutX="290.0" layoutY="406.0" mnemonicParsing="false" onAction="#handleButtonDown" prefHeight="30.0"
                prefWidth="60.0" text="DOWN"/>
    </children>
</AnchorPane>
